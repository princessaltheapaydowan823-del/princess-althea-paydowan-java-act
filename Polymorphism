// Parent class
class Vehicle {
    private String brand;
    private String model;
    private int year;

    // Constructor
    public Vehicle(String brand, String model, int year) {
        this.brand = brand;
        this.model = model;
        this.year = year;
    }

    public void startEngine() {
        System.out.println("Engine is now running...");
    }

    // Getter methods
    public String getBrand() {
        return brand;
    }

    public String getModel() {
        return model;
    }

    public int getYear() {
        return year;
    }
}

// Child class
class Car extends Vehicle {
    private int maxSpeed;
    private String fuelType;

    // Constructor
    public Car(String brand, String model, int year, int maxSpeed, String fuelType) {
        super(brand, model, year);  // call parent constructor
        this.maxSpeed = maxSpeed;
        this.fuelType = fuelType;
    }

    public void showDetails() {
        System.out.println("Brand: " + getBrand());
        System.out.println("Model: " + getModel());
        System.out.println("Year: " + getYear());
        System.out.println("Max Speed: " + maxSpeed + " km/h");
        System.out.println("Fuel Type: " + fuelType);
    }
}

public class Main {
    public static void main(String[] args) {

        Car car = new Car("Honda", "Civic", 2022, 180, "Gasoline");

        car.startEngine();
        car.showDetails();
    }
}
You sent
// Parent class
class Transport {

    // Overloading (Compile-Time Polymorphism)
    void details() {
        System.out.println("General transport information.");
    }

    void details(String brand) {
        System.out.println("Brand: " + brand);
    }

    void details(String brand, int year) {
        System.out.println("Brand: " + brand + " | Year: " + year);
    }

    void details(String brand, String model, String color, int year, String fuel) {
        System.out.println("Brand: " + brand);
        System.out.println("Model: " + model);
        System.out.println("Color: " + color);
        System.out.println("Year: " + year);
        System.out.println("Fuel: " + fuel);
    }

    // Runtime polymorphism (Overriding)
    void maxSpeed() {
        System.out.println("Speed depends on the transport type.");
    }
}

// Child class 1
class FourWheeler extends Transport {
    @Override
    void maxSpeed() {
        System.out.println("Four-wheeler speed: around 180 km/h.");
    }
}

// Child class 2
class TwoWheeler extends Transport {
    @Override
    void maxSpeed() {
        System.out.println("Two-wheeler speed: around 160 km/h.");
    }
}

public class Main {
    public static void main(String[] args) {

        Transport t;

        // Four-Wheeler
        t = new FourWheeler();
        t.details();
        t.details("Honda");
        t.details("Honda", 2023);
        t.details("Honda", "Civic RS Turbo", "White", 2023, "Gasoline");
        t.maxSpeed();

        System.out.println();

        // Two-Wheeler
        t = new TwoWheeler();
        t.details();
        t.details("Suzuki");
        t.details("Suzuki", 2022);
        t.details("Suzuki", "GSX-R150", "Blue", 2022, "Unleaded");
        t.maxSpeed();
    }
    }
